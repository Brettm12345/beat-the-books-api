{
  "name": "beat-the-books-api",
  "version": "1.0.0",
  "scripts": {
    "build": "sls package",
    "start": "sls offline",
    "deploy": "sls deploy",
    "lint": "eslint --ext .ts ./src ./tests",
    "lint:fix": "yarn -s lint --fix",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env NODE_ENV=development ts-node -T -r dotenv/config -r tsconfig-paths/register ./src/schema",
    "generate:nexus-prisma": "npx nexus-prisma-generate --client ./src/generated/prisma-client --output ./src/generated/nexus-prisma",
    "generate": "yarn run generate:prisma && yarn run generate:nexus && yarn run generate:nexus-prisma",
    "test": "jest",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch"
  },
